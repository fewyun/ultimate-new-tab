{
  "name": "Ultimate New Tab",
  "version": "0.4",
  "description": "Works with Smooth Gestures. A custom new tab page that supports theming, tab organization, bookmarks, history, and speeddial.",
  "minimum_chrome_version": "8",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "background_page": "background.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [ "tabs", "bookmarks", "history", "http://*/*", "https://*/*", "unlimited_storage", "chrome://favicon/", "management" ]
}
